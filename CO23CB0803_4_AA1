<html>
    <head>
    </head>
</html>
<body>
    <script>

var randomNumber1 = Math.random()
var randomNumber2= Math.random()
var randomNumber3= Math.random()
var randomNumber4= Math.random()
var randomNumber5= Math.random()

document.write(randomNumber1);
document.write("<br>");
document.write(randomNumber2);
document.write("<br>");
document.write(randomNumber3);
document.write("<br>");
document.write(randomNumber4);
document.write("<br>");
document.write(randomNumber5);

var largest_number = Math.max(randomNumber1,randomNumber2,randomNumber3,randomNumber4,randomNumber5);
document.write("<br>");
document.write("Largest of the generated numbers is ", largest_number);


var smallest_number = Math.min(randomNumber1,randomNumber2,randomNumber3,randomNumber4,randomNumber5);
document.write("<br>");
document.write("smallest of the generated numbers is ", smallest_number);


var rounded_number = Math.round(largest_number);
document.write("<br>");
document.write("Largest number rounded off is ", rounded_number);


var floored_number = Math.floor(smallest_number);
document.write("<br>");
document.write("Smallest number floored off is ", floored_number);

</script>
</body>
</html>
